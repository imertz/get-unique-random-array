{
  "name": "get-unique-random-array",
  "version": "1.0.5",
  "description": "Get unique random array from an array",
  "repository": "mar-i-nda/get-unique-random-array",
  "main": "dist/bundle.js",
  "module": "dist/index.js",
  "scripts": {
    "rollup-cjs": "rollup dist/index.js --output.format cjs --output.file dist/bundle.js",
    "prepublishOnly": "npm test && npm run build",
    "build": "babel src --out-dir dist --ignore '**/*.test.js' && npm run rollup-cjs",
    "test": "xo && ava",
    "test-watch": "npm test -- --watch"
  },
  "author": "Marinda <hello@marinda.me> (http://marinda.me/)",
  "keywords": [
    "random array",
    "unique random array"
  ],
  "license": "MIT",
  "devDependencies": {
    "ava": "0.25.0",
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-prettier": "2.6.0",
    "prettier": "1.10.2",
    "rollup": "0.56.1",
    "xo": "0.18.2"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "xo": {
    "prettier": true,
    "extends": [
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "dependencies": {}
}
